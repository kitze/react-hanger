  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "generate-dts": "tsc -p tsconfig-dts.json",
    "prebuild": "rimraf dist",
    "build": "npm run generate-dts && rollup -c",
